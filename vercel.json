{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|build|storage)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "public": true,
    "buildCommand": "composer install && php artisan key:generate && php artisan migrate --force && php artisan config:cache && php artisan route:cache && php artisan view:cache && vite build",
    "outputDirectory": "public",
    "env": {
        "APP_NAME": "praktikum-pkpl",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://praktikum-pkpl.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH": "node",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "pr1.h.filess.io",
        "DB_PORT": "3307",
        "DB_DATABASE": "praktikumpkpl_happenofor",
        "DB_USERNAME": "praktikumpkpl_happenofor",
        "DB_PASSWORD": "5ceca3131f3139fb38c43c76158ddf24304bcf04"
    }
}
